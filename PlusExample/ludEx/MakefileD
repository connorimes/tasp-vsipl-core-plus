## SPAWARSYSCEN D881 

CC=cc
INCLUDE=-I../../include -I../../include/private
DEFINES=-DVSIP_DEVELOPMENT_MODE
OPTIONS=-O2 -Wall -ansi -pedantic
LIBRARY=-L../../lib
libraries= -lDvsip_c \
-lm

COMPILE=$(CC) $(INCLUDE) $(LIBRARY) $(DEFINES) $(OPTIONS)

all: ludEx lud2Ex clud2Ex

ludEx:ludEx.c
	$(COMPILE) -o ludEx ludEx.c $(libraries)

lud2Ex:lud2Ex.c
	$(COMPILE) -o lud2Ex lud2Ex.c $(libraries)

clud2Ex:clud2Ex.c
	$(COMPILE) -o clud2Ex clud2Ex.c $(libraries)

clean:
	rm -f ludEx lud2Ex clud2Ex *.exe
